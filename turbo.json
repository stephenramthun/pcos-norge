{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "db:migrate"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "test": {},
    "lint": {},
    "dev": {
      "cache": false,
      "dependsOn": [
        "db:generate"
      ]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "cms:build": {
      "cache": false
    },
    "cms:deploy": {
      "cache": false
    },
    "clean": {}
  }
}
