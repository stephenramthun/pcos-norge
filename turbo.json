{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "db:migrate"],
      "outputs": [".next/**"]
    },
    "test": {},
    "cy:test": {
      "cache": false,
      "dependsOn": ["db:generate"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["db:generate"]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "clean": {}
  }
}
